{
  "name": "geohashing",
  "version": "2.0.0",
  "description": "TypeScript-written Geohash library for Node.js and the browser",
  "keywords": [
    "nodejs",
    "javascript",
    "typescript",
    "geohash",
    "geocoding",
    "geolocation",
    "geospatial",
    "geojson"
  ],
  "author": "Arseny Buravlev <arseny.buravlyov@gmail.com>",
  "repository": "https://github.com/arseny034/geohashing",
  "homepage": "https://github.com/arseny034/geohashing#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.esm.mjs",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./lib/index.esm.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib/**/*",
    "package.json"
  ],
  "scripts": {
    "build": "microbundle -f esm,cjs --raw=true --tsconfig tsconfig.build.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint src/** test/** --fix",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
